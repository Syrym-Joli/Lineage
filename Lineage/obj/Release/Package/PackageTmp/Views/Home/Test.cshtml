
@{
    Layout = null;       // "~/Views/Shared/_Layout.cshtml"
}

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="http://resources/demos/style.css">
    <link rel="stylesheet" href="http://cdn.metroui.org.ua/css/metro.min.css">
    <link rel="stylesheet" href="http://cdn.metroui.org.ua/css/metro-icons.min.css">
    <link rel="stylesheet" href="http://cdn.metroui.org.ua/css/metro-font-cdn.css">
    
    <style>
        #draggable {
            width: 150px;
            height: 150px;
            padding: 0.5em;
            background: #808080
        }
    </style>
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script>
        $(function () {
            $("#draggable").draggable();
        });
    </script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/winjs/4.4.0/css/ui-light.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/winjs/4.4.0/js/base.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/winjs/4.4.0/js/ui.js"></script>
    
    <script src="http://cdn.metroui.org.ua/js/metro.min.js"></script>
</head>
<body>
    <div class="app-bar fixed-top">
        <a class="app-bar-element">
            <span id="toggle-tiles-dropdown" class="mif-arrow-left mif-2x" onclick="nazad()"></span>
            <ul class="app-bar-menu">
                <li><a href="">Home</a></li>
                <li><a href="">Support</a></li>
                <li><a href="">Help</a></li>

            </ul>
        </a>
        <li class="place-right no-hovered">
            <form>
                <div class="input-control text" style="width: 250px; margin-right: 10px">
                    <input type="text" placeholder="Search...">
                    <button class="button"><span class="mif-search"></span></button>
                </div>
            </form>
        </li>
    </div>

    <script>
        function nazad() { document.location.href = "/home"; }
    </script>


    <div id="draggable">
        <button class="button cycle-button">+</button>
    </div>


</body>
</html>